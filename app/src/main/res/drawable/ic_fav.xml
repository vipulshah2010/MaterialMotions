<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <size
                android:width="80dp"
                android:height="80dp" />
            <solid android:color="#66FFFFFF" />
        </shape>
    </item>

    <item
        android:bottom="8dp"
        android:end="8dp"
        android:start="8dp"
        android:top="8dp">
        <shape android:shape="oval">
            <size
                android:width="60dp"
                android:height="60dp" />
            <solid android:color="#FFFFFF" />
        </shape>
    </item>

</layer-list>